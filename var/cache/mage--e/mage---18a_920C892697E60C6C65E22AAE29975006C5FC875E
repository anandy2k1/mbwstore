  jQuery(document).ready(function() {     
        var owl = jQuery("#owl-demo3");
	    owl.owlCarousel({
						autoPlay : true,
			stopOnHover : true,
						items : 5
		});
		// Custom Navigation Events
		jQuery(".next3").click(function(){
			owl.trigger('owl.next');
		})
		jQuery(".prev3").click(function(){
			owl.trigger('owl.prev');
		})     
	 });
</script>
